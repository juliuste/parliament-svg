	"devDependencies": {
		"depcheck": "^1.4.2",
		"eslint": "^7.32.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.1.0",
		"hast-to-hyperscript": "^10.0.1",
		"hast-util-sanitize": "^4.0.0",
		"hast-util-to-html": "^8.0.2",
		"tape": "^5.3.1",
		"virtual-dom": "^2.1.1",
		"webpack": "^5.56.1",
		"webpack-cli": "^4.8.0"
	},
	"engines": {
		"node": ">=14"
	}
