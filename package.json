{
	"name": "parliament-svg",
	"version": "2.1.4",
	"description": "Generate parliament charts as virtual-dom SVG.",
	"keywords": [
		"chart",
		"parlament",
		"parliament",
		"svg",
		"wiki",
		"wikipedia"
	],
	"homepage": "https://github.com/juliuste/parliament-svg",
	"bugs": "https://github.com/juliuste/parliament-svg/issues",
	"repository": "juliuste/parliament-svg",
	"license": "ISC",
	"author": "Julius Tens <mail@juliustens.eu>",
	"files": [
		"src/*"
	],
	"main": "./src/index.js",
	"module": "./src/index.js",
	"scripts": {
		"build": "npm run bundle && npm run minify",
		"bundle": "mkdir -p docs/bundle && browserify docs/index.js > docs/bundle/index.js",
		"check-deps": "depcheck --ignore-dirs='bundle'",
		"fix": "npm run lint -- --fix",
		"lint": "eslint src test example docs/index.js",
		"minify": "uglifyjs -mc -- docs/bundle/index.js > docs/bundle/index.min.js",
		"prepublishOnly": "npm test",
		"test": "npm run lint && npm run check-deps && tape test/*.js"
	},
	"dependencies": {
		"lodash": "^4.17.21",
		"sainte-lague": "^2.1.1",
		"virtual-hyperscript-svg": "^2.0.0"
	},
	"devDependencies": {
		"browserify": "^17.0.0",
		"depcheck": "^1.4.2",
		"eslint": "^7.32.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.1.0",
		"eslint-plugin-standard": "^4.0.0",
		"tape": "^5.3.1",
		"uglify-js": "^3.14.2",
		"virtual-dom": "^2.1.1",
		"virtual-dom-stringify": "^3.0.1"
	},
	"engines": {
		"node": ">=14"
	}
}
